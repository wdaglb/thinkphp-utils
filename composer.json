{
    "name": "ke/thinkphp-utils",
    "description": "the thinkphp utils",
    "type": "library",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1"
    },
    "autoload": {
        "psr-4": {
            "ke\\utils\\": "src/"
        }
    },
    "require-dev": {
        "topthink/framework": "5.1.*"
    },
    "config": {
        "allow-plugins": {
            "topthink/think-installer": true
        }
    }
}
